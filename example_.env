EXCHANGES="binance,coinbase"
DEFAULT_EXCHANGE="binance"

BINANCE_API_KEY="Your Binance API key"
BINANCE_API_SECRET="Your Binance API secret"

COINBASE_API_KEY="Your Coinbase API key"
COINBASE_API_SECRET="Your Coinbase API secret"

# Internal API
API_ENDPOINT_PRICE="http://localhost/api/prices"

# Available modes:
# "trade" to live watch and record markets
# "live" to live trade through WebSocket
# "backtest" to test a strategy for a given symbol pair and period
DEFAULT_MODE="trade"
DEFAULT_STRATEGY="watcher"

# Allow trading "test" mode or "real" trading
DEFAULT_TRADING_MODE="test"

# Default candle size in seconds
DEFAULT_TRADE_CANDLESTICK_INTERVAL=60

# Currency and asset
DEFAULT_CURRENCY="BTC"
DEFAULT_ASSET="GBP"

# Default backtest period: string in UTC format or timestamp in milliseconds
DEFAULT_PERIOD_START="1 day ago UTC"
DEFAULT_PERIOD_END=""
