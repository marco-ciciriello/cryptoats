DEFAULT_ASSET="USDT"

# Default backtest period: string in UTC format or timestamp in milliseconds
DEFAULT_PERIOD_START="1 day ago UTC"
DEFAULT_PERIOD_END=""

DATABASE_URL="Path to Postgres db"
